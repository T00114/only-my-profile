{
  "name": "only-my-profile",
  "version": "1.0.0",
  "description": "React + grommet ES6 syntax app",
  "main": "index.js",
  "scripts": {
    "test": "npm-run-all -p eslint nsp",
    "nsp": "nsp check",
    "eslint": "eslint .",
    "eslint:fix": "eslint --fix .",
    "prettier": "find ./src ./containers ./components -name '*.js' -o -name '*.jsx' | xargs prettier-eslint --write",
    "start": "webpack-dev-server --history-api-fallback --hot --inline --progress --colors",
    "build:debug": "webpack --display-error-details --progress --colors",
    "build": "NODE_ENV=production webpack --progress --colors",
    "build:win": "set NODE_ENV=production && webpack --progress --colors",
    "ncu": "ncu -a"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/j138/only-my-profile"
  },
  "keywords": [
    "React",
    "grommet",
    "webpack",
    "bable"
  ],
  "author": "https://github.com/",
  "license": "MIT",
  "bugs": {
    "url": ""
  },
  "greenkeeper": {
    "ignore": [
      ""
    ]
  },
  "homepage": "",
